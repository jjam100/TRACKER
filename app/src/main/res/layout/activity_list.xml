<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2f2f2"
    tools:context=".List">

    <TextView
        android:id="@+id/list_head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="70dp"
        android:text="TRACKING LIST"
        android:textColor="@android:color/black"
        android:textSize="30sp" />

    <Button
        android:id="@+id/tracker_button1"
        android:layout_width="249dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/list_head"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:text="감정" />


    <Button
        android:id="@+id/tracker_button2"
        android:layout_width="249dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tracker_button1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:text="트래커2" />


    <Button
        android:id="@+id/tracker_button3"
        android:layout_width="249dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tracker_button2"
        android:layout_marginTop="19dp"
        android:layout_centerHorizontal="true"
        android:text="트래커3" />

    <Button
        android:id="@+id/tracker_button4"
        android:layout_width="249dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tracker_button3"
        android:layout_marginTop="19dp"
        android:layout_centerHorizontal="true"
        android:text="트래커4" />

    <Button
        android:id="@+id/tracker_button5"
        android:layout_width="249dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tracker_button4"
        android:layout_marginTop="19dp"
        android:layout_centerHorizontal="true"
        android:text="트래커5" />

</RelativeLayout>